{
  "name": "@raha/homepage",
  "version": "0.0.1",
  "description": "Google AMP static site for the Raha Foundation",
  "repository": "git@github.com:rahafoundation/raha-homepage.git",
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "amphtml-validator": "^1.0.23",
    "concurrently": "^3.5.1",
    "gh-pages": "^1.1.0",
    "live-server": "^1.2.0",
    "static-i18n": "^0.1.24",
    "watch": "^1.0.2"
  },
  "scripts": {
    "build": "./scripts/build",
    "deploy:force": "yarn build && gh-pages -d build/",
    "deploy": "./scripts/deploy",
    "lint": "./scripts/lint",
    "watch:build": "watch \"yarn build\" src/ locales/",
    "serve":
      "concurrently --kill-others \"live-server ./build\" \"yarn watch:build\""
  }
}
